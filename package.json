{
  "name": "bkradio-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon --experimental-modules --es-module-specifier-resolution=node index.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node index.js",
    "deploy:delete-staging-tag": "git fetch --tags && git push origin --delete $(git tag -l 'staging-*') || true && git fetch origin --prune 'refs/tags/*:refs/tags/*'",
    "staging:deploy": "yarn deploy:delete-staging-tag && TAG=`echo staging-$(date +%Y%m%d-%H%M%S)` && git tag $TAG && git push origin $TAG"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.2.0",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "express-promise-router": "^4.1.1",
    "firebase": "^9.8.4",
    "firebase-admin": "^11.0.0",
    "fs": "^0.0.1-security",
    "got": "^12.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.1",
    "knex-paginate": "^3.0.0",
    "kue": "^0.11.6",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nnn-router": "^1.1.5",
    "node-mocks-http": "^1.11.0",
    "pdf-parse": "^1.1.1",
    "slugify": "^1.6.5",
    "statuses": "^2.0.1",
    "to-case": "^2.0.0"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "nodemon": "^2.0.15"
  },
  "_moduleAliases": {
    "@root": "./",
    "@routes": "./routes",
    "@helpers": "./helpers"
  }
}
